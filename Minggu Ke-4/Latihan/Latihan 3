package Praktikum_Minggu_4;

/**
 *
 * @author Bravo 15
 */
public class Praktikum_3 {
    public static void main(String [] args){
        
        for (int i = 0; i <= 5; i++) {
            for (int j = 0; j <= i; j++){
                System.out.print("*");
            }
            System.out.println("");
        }
    }
    
}
