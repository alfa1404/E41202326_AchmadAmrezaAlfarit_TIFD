package Praktikum_Minggu_4;

/**
 *
 * @author Bravo 15
 */
public class Praktikum_1 {
    public static void main(String [] args){
       int i = 0;
       
       while (i <=10) {
           // blok kode yang akan di ulang
           System.out.println("Perulangan ke-" + i);
    // increment nilai i
    i++;
       }
   }
    
}
